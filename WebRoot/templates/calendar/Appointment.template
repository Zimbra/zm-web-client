                    <td class='ZmApptTabViewPageField'><$= ZmMsg.locationLabel $></td>
                    <td colspan=2 id='${id}_location'></td>
                </tr>
                <tr>
                    <td></td>
                    <td colspan='3'><div id='${id}_suggest_location' style="margin-top:-4px;" class='fakeAnchor'><$= ZmMsg.suggestALocation $></div></td>
