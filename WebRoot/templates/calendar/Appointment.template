			<!-- key/legend section -->
			<table class='ZmGraphKey'>
				<tr>
					<td class='ZmGraphKeyHeader'><$= ZmMsg.key $></td>
				</tr>
				<tr>
					<td class='ZmGraphKeyBody'>
						<table>
							<tr>
								<td><div class='ZmGraphKeyColorBox ZmScheduler-free'></div></td>
