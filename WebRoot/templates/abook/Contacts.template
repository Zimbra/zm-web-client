						<tr>
							<td class='Label nobreak'><$= ZmMsg.showNames $>&nbsp;</td>
							<td id='${id}_folders'></td>
						</tr>
					</table>
				<$ } $>
			</td>
		</tr>
	</table>
	<div id='${id}_results' class='contactSearchResultsDiv'></div>
</template>

