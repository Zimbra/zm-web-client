</template>

<template id='abook.Contacts#SplitView_group'>
	<$
		var contact = data.contact;
		var imageUrl = data.imageUrl;
		var defaultImageUrl = data.defaultImageUrl;
		var email = data.email;
		var title = data.title;
		var fullName = data.fullName;
		var phone = data.phone;
		var location = data.location;
		var imgClassName = contact && contact.isDistributionList() ? 'DistributionList' : contact && contact.isGroup() ? 'Group' : 'Contact';
		var quickAdd = "id='" + data.quickAddId + "'";
		var isEdit = data.isEdit;
		var delId = "id='" + data.delButtonId + "'";
	$>
	<table role="presentation" class='ZmContactGroupItemTable'>
		<tr>
			<td class='ZmContactGroupItemImage' width=42>
				<$ if (imageUrl) { $>
					<img src='<$= imageUrl $>'
						<$ if (defaultImageUrl) { $>
							onerror='this.onerror=null;this.src="${defaultImageUrl}";'
						<$ } $>
					/>
				<$ } else { $>
					<$= AjxImg.getImageHtml({
						imageName: imgClassName
					}) $>
				<$ } $>
			</td>
			<td class='ZmContactGroupItemContent'>
				<$ if (fullName) {$>
					<div><strong><$=fullName$></strong></div>
				<$ } $>
				<$ if (email) {$>
					<div>
						<$=email$>
						<$ if (data.isInline) {$><$=AjxImg.getImageHtml("Plus", "", quickAdd)$><$ }$>
					</div>
				<$}$>
			</td>
		<$ if (isEdit) {$>
			<td width='20' valign='middle' class='ZmContactGroupItemAction'><$=AjxImg.getImageHtml("Close", "", delId)$></td>
		<$}$>
		</tr>
	</table>
</template>

<template id='abook.Contacts#SplitView_header'>
	<$
		var contact = data.contact;
		var imageUrl = contact.getImageUrl();
		var imageLabel = contact.isDistributionList() ? ZmMsg.distributionList : contact.isGroup() ? ZmMsg.group : ZmMsg.contact;
		var defaultImageUrl = data.defaultImageUrl;
		var accountName = appCtxt.multiAccounts && contact.account && contact.account.getDisplayName();
		var tagsId = data.id + '_tags_contact'; 
		var imgClassName = contact.isDistributionList() ? 'DistributionList' : contact.isGroup() ? 'Group' : 'Contact';
		var isEdit = data.isEdit;
	$>
	<div class='ZmContactHeaderRow headerRow' id='${id}_headerRow'>
		<div class='ZmContactHeaderMain'>
			<table role="presentation">
				<tr>
					<td class='ZmContactHeaderImage'>
						<$ if (imageUrl) { $>
							<img src='<$= imageUrl $>' alt='<$= imageLabel $>'
								<$ if (defaultImageUrl) { $>
									onerror='this.onerror=null;this.src="${defaultImageUrl}";'
								<$ } $>
							/>
						<$ } else { $>
							<$= AjxImg.getImageHtml({
								imageName: imgClassName, altText: imageLabel
							}) $>
						<$ } $>
					</td>
					<td class='ZmContactHeaderInfo rowValue'>
						<$= AjxTemplate.expand("#SplitView_headerName", data) $>
						<$= AjxTemplate.expand("#SplitView_headerJobInfo", data) $>
						<$ if (data.dlInfo && data.dlInfo.description) { $>
						<div class='ZmContactHeaderText'><$= AjxStringUtil.htmlEncode(data.dlInfo.description) $></div>
						<$ } $>
						<$ if (!isEdit) {$>
							<div class='ZmContactHeaderTags' id='<$= tagsId $>'></div>
						<$ } $>
					</td>
				</tr>
				<!-- Edit Contact Group Tags -->
				<tr>
					<td></td>
					<td><div id='${id}_tags' class='ZmContactHeaderTags'></div></td>
				</tr>
			</table>
		</div>
		<div class='ZmContactHeaderLocation rowType'>
			<$ if (accountName) { $>
				<table role="presentation" class='ZPropertySheet' cellspacing='6'>
					<tr>
						<td class="ZmContactHeaderLocationIcon"><$= ZmMsg.accountLabel $></td>
						<td class="ZmContactHeaderLocationName"><$= AjxStringUtil.htmlEncode(accountName) $></td>
					</tr>
				</table>
			<$ } $>
			<$ if (!isEdit) { $>
				<$ if (data.addrbook) { $>
					<table role="presentation" class='ZPropertySheet' cellspacing='6'>
						<tr>
							<td class="ZmContactHeaderLocationIcon">
								<$= AjxImg.getImageHtml({
									imageName: data.addrbook.getIconWithColor(),
									altText: ZmMsg.locationLabel
								}) $>
							</td>
							<td class="ZmContactHeaderLocationName"><$= data.addrbook.getName() $></td>
						</tr>
					</table>
				<$ } else { $>
					<table role="presentation" class='ZPropertySheet' cellspacing='6'>
						<tr>
							<td class="ZmContactHeaderLocationIcon">
								<$= AjxImg.getImageHtml('GALFolder') $>
							</td>
							<td class="ZmContactHeaderLocationName">
								<$= ZmMsg.GAL $>
							</td>
						</tr>
					</table>
				<$ } $>
			<$ } else { $>
				<!-- If DL edit, don't show location -->
				<$ if(!contact.isDistributionList()) { $>
					<table role="presentation" class='ZPropertySheet' cellspacing='6'>
						<tr>
							<td id='${id}_locationLabel' class='ZmContactHeaderLocationLabel'>
								<$= ZmMsg.locationLabel $>
							</td>
							<td id='${id}_LOCATION_FOLDER' class='ZmContactHeaderLocationName'></td>
						</tr>
					</table>
				<$ } $>
			<$ } $>
		</div>
	</div>
</template>

<template id='abook.Contacts#SplitView_headerName'>
	<$
		var contact = data.contact;
		var isPhonetic = appCtxt.get(ZmSetting.PHONETIC_CONTACT_FIELDS);
		var fullnameHtml = contact.getFullNameForDisplay(isPhonetic);
		if (!isPhonetic) {
			fullnameHtml = AjxStringUtil.htmlEncode(fullnameHtml);
		}
		var nickname = contact.getAttr(ZmContact.F_nickname);
		var isEdit = data.isEdit;
		if (isEdit && data.contact.isDistributionList()) {
			fullnameHtml = '';
		}
	$>
	<div id='${id}_contactName' class='ZmContactTitle <$= data.isInTrash ? "Trash" : "" $>'><$= fullnameHtml $></div>
	<$ if (nickname && !isEdit) { $>
		<$ if(contact.isGroup()) { $>
			<div class='ZmContactTitle'><$= AjxStringUtil.htmlEncode(nickname) $></div>
		<$ } else { $>
			<div class='ZmContactSubTitle'>&ldquo;<$= AjxStringUtil.htmlEncode(nickname) $>&rdquo;</div>
		<$ } $>
	<$ } $>
	<$ else if (isEdit) {$>
		<div class='contactHeader2'>
			<$	if (data.contact.isDistributionList()) {
			$>
			<table role="presentation">
				<tr>
					<td class='contactHeader2'>
			<$
					if (data.usernameEditable) { $>
						<div id='${id}_groupNameParent'></div>
			<$		} else { $>
						<$= AjxStringUtil.htmlEncode(data.username)$>
			<$		}
			$>
					</td>
					<td class='contactHeader2'>&nbsp;@
			<$
					if (data.domainEditable) { $>
						<input type='text' autocomplete='off' size='20' id='${id}_groupNameDomain' class='Text'>
			<$		} else { $>
						<$= AjxStringUtil.htmlEncode(data.domain)$>
			<$		} $>
					</td>
				</tr>
			</table>
			<$ } else { $>
					<div id='${id}_groupNameParent'></div>
			<$ } $>
		</div>
	<$ } $>
</template>

<template id='abook.Contacts#SplitView_headerJobInfo'>
	<$
		var contact = data.contact;
		var isPhonetic = appCtxt.get(ZmSetting.PHONETIC_CONTACT_FIELDS);
		var company = contact.getAttr(ZmContact.F_company);
		var title = contact.getAttr(ZmContact.F_jobTitle);
		var department = contact.getAttr(ZmContact.F_department);
	$>
	<$ if (title || department) { $>
		<div class='ZmContactHeaderJobInfo'>
			<$ if (title) { $>
				<span><$= AjxStringUtil.htmlEncode(title) $></span>
			<$ } $>
			<$ if (department) { $>
				<$ if (title) { $>&nbsp;&ndash;&nbsp;<$ } $>
				<span><$= AjxStringUtil.htmlEncode(department) $></span>
			<$ } $>
			<$ if (company) {
				var phoneticCompany = isPhonetic && contact.getAttr(ZmContact.F_phoneticCompany);
				var companyHtml = AjxStringUtil.htmlRubyEncode(company, phoneticCompany);
				$>
				<$ if (title) { $>&comma;&nbsp;<$ } $>
				<span><$= companyHtml$></span>
			<$ } $>
		</div>
	<$ } $>
</template>

