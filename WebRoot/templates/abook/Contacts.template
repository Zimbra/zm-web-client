				<div id='${id}_splitview' style='position:relative;width:100%;height:100%;'>
					<div id='${id}_listview' style='position:absolute;width:350px;height:100%;left:0;'></div>
					<div id='${id}_sash' style='position:absolute;height:100%;left:350px;'></div>
