<template id='abook.Contacts#SplitView_address_value'>
	<tr>
		<td>
			<div class='ZmContactRowWrapper'>
				<div class='ZmContactRowLabel' id='${labelId}_label'>
						<!-- only show the first label visually -->
						<label for='${labelId}_label' class='<$= data.first ? "" : "ScreenReaderOnly" $>'>
							${label}
						</label>
				</div>
				<div class='ZmContactRowValueWrapper'>
					<div id='${labelId}_value' class='ZmContacRowValue rowValue' aria-labelledby='${labelId}_label ${labelId}_type'>
						<$ var address = data.address;
							if (address.Street) { $>
								<$=address.Street$>
							<$ }
							if (address.City || address.State || address.PostalCode) { $>
								<$ if (address.Street) { $>,&nbsp;<$ } $>
								<$=address.City$>
								<$ if (address.City && address.State) { $>,&nbsp;<$ } $>
								<$=address.State$>
								<$ if (address.City || address.State) { $>&nbsp;<$ } $>
								<$=address.PostalCode$>
							<$ }
							if (address.Country) { $>
								<$ if (address.Street || address.City || address.State || address.PostalCode) { $>,&nbsp;<$ } $>
								<$=address.Country$>
							<$ } 
