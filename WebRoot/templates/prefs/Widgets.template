<template id="prefs.Widgets#infoBox" xml:space="strip">
<div id="${id}_infoBox" class='ZInfoBox'>
	<table role="presentation" id='${id}_infoBox_outer' width='100%'>
	<tr>
		<td rowspan=2 style='vertical-align:top'><div class='ImgInformation_32'></div></td>
		<td><div id="${id}_infoBox_close" class="ZInfoTitleClose" onclick='Dwt.toggle("${id}_infoBox_container")'><$= ZmMsg.close $></div><div id="${id}_title" class='ZInfoTitle'>${_labels.infoTitle}</div></td>
	</tr>
	<tr>
		<td id="${id}_body">${_labels.infoContents}</td>
	</tr>
	</table>
</div>
</template>
