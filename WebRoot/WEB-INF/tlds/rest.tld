    <tag-file>
        <name>calendarViewBottomToolbar</name>
        <path>/WEB-INF/tags/rest/restCalendarViewBottomToolbar.tag</path>
    </tag-file>

    <tag-file>
        <name>displayAppointment</name>
        <path>/WEB-INF/tags/rest/restDisplayAppointment.tag</path>
    </tag-file>

    <tag-file>
        <name>apptView</name>
        <path>/WEB-INF/tags/rest/restApptView.tag</path>
    </tag-file>



</taglib>

        