       <name>view</name>
       <path>/WEB-INF/tags/rest/restView.tag</path>
    </tag-file>

   <tag-file>
       <name>status</name>
       <path>/WEB-INF/tags/rest/restStatus.tag</path>
    </tag-file>

    <tag-file>
        <name>calendar</name>
        <path>/WEB-INF/tags/rest/restCalendar.tag</path>
    </tag-file>

    <tag-file>
        <name>document</name>
        <path>/WEB-INF/tags/rest/restDocument.tag</path>
    </tag-file>    

    <tag-file>
        <name>slideView</name>
        <path>/WEB-INF/tags/rest/restSlideView.tag</path>
