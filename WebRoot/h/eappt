                                    <c:otherwise>
                                        <app:button name="actionApptDelete" src="startup/ImgDelete.gif" tooltip="actionApptDeleteTT" text="delete"/>
                                    </c:otherwise>
                                </c:choose>
                            </c:if>

                            <c:if test="${canDelete and not isInstance}">
                                <td><div class='vertSep'></div></td>
