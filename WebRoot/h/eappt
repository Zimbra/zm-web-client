                            <td><div class='vertSep'></div></td>
                            <c:choose>
                                <c:when test="${isNewAppt}">
                                    <app:button name="actionCancel" src="common/ImgCancel.gif" tooltip="cancel" text="cancel"/>
                                </c:when>
                                <c:otherwise>
                                    <app:button name="actionCancel" src="common/ImgClose.gif" tooltip="close" text="close"/>        
                                </c:otherwise>
                            </c:choose>


                            <c:set var="editrecur" value="${(empty appt or not appt.exception) and not compose.useInstance}"/>
                            <td><div class='vertSep'></div></td>
