                                    <c:forEach begin="0" end="47" step="1" var="indx">
                                        <c:set var="curTm" value="${indx*30}"/>
                                       <c:set var="hasappt" value="${zm:hasAnyAppointments(attFreeBusy, today.timeInMillis + curTm*60*1000, today.timeInMillis + (curTm+30)*60*1000)}"/>
                                        <c:if test="${hasappt}">
