From 85a2b8ee6778641ffb62c80f13edee4ff1f0506f Mon Sep 17 00:00:00 2001
From: Dan Villiom Podlaski Christiansen <dan@cabo.dk>
Date: Mon, 13 Oct 2014 12:21:13 +0200
Subject: [PATCH] add grunt-cli to list of dependancies

This allows a single 'npm install' to install everything required to
build TinyMCE.
---
 package.json | 1 +
 1 file changed, 1 insertion(+)

diff --git a/package.json b/package.json
index 217eb84..bac4efc 100644
--- a/package.json
+++ b/package.json
@@ -16,6 +16,7 @@
 	},
 	"devDependencies": {
 		"grunt": "~0.4.5",
+		"grunt-cli": "~0.1.13",
 		"grunt-contrib-clean": "~0.6.0",
 		"grunt-contrib-connect": "~0.8.0",
 		"grunt-contrib-jshint": "~0.10.0",
